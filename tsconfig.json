{
    "compilerOptions": {
        "target": "ES5",
        "lib": ["DOM", "DOM.Iterable","ESNext"],
        "skipLibCheck": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "ESNext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "jsx": "react-jsx"
    },
    "include": ["src"]
}

